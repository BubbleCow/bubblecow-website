


<div class="row">
  <div class="col-sm-6">
    <h1>Dev_edits</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_dev_edit_path, class: 'btn btn-primary' do %>
    Add New Dev edit
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Status</th>
            <th>User</th>
            <th>Accepted date</th>
            <th>Rejected date</th>
            <th>Invoice due date</th>
            <th>Invoice paid date</th>
            <th>Return date</th>
            <th>Due date</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @dev_edits.each do |dev_edit| %>
        <%= content_tag :tr, id: dom_id(dev_edit), class: dom_class(dev_edit) do %>
          

                      <td><%= dev_edit.status %></td>
                      <td><%= dev_edit.user_id %></td>
                      <td><%= dev_edit.accepted_date %></td>
                      <td><%= dev_edit.rejected_date %></td>
                      <td><%= dev_edit.invoice_due_date %></td>
                      <td><%= dev_edit.invoice_paid_date %></td>
                      <td><%= dev_edit.return_date %></td>
                      <td><%= dev_edit.due_date %></td>
          
          
            <td><%= link_to 'Show', dev_edit %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
