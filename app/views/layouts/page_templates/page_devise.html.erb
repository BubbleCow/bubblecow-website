<!DOCTYPE html>
<html>

    <%= render 'shared/layout/head' %>

  <body>

    <!-- Google tag manager -->
    <%= render 'shared/layout/google_tag_manager' %>

    <!-- navigation -->
    <%= render 'shared/layout/navbar' %>

    <!-- Notices -->
    <%= render 'shared/layout/notices' %>

    <!-- Main -->
    <main>
        <div class="container my-5">
            <div class="row justify-content-center">
                <div class="col-12 col-sm-8 col-md-6 col-lg-4">
                    <%= yield %>
                </div>
            </div>
        </div>
    </main>

  </body>

</html>




