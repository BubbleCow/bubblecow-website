<div class="page-header">
  <%= link_to mailing_lists_path, class: 'btn btn-default' do %>
    All Mailing_lists
  <% end %>
  <%= link_to edit_mailing_list_path(@mailing_list), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show mailing_list</h1>
</div>

<dl class="dl-horizontal">
  <dt>Email:</dt>
  <dd><%= @mailing_list.email %></dd>

  <dt>Tag:</dt>
  <dd><%= @mailing_list.tag %></dd>

</dl>
