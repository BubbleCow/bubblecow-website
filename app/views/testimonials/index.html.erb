<main class="page" id="testimonials">

  <div class="page-header">
    <h1>Testimonials</h1>
  </div>

  <% @testimonials.each do |testimonial| %>
      <div class="box mb-3">
        <p><%= testimonial.testimonial_text %></p>
        <h6><%= testimonial.writer.titleize %></h6>
      </div>
  <% end %>

</main>
